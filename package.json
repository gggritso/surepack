    "@types/pluralize": "^0.0.33",
    "@types/yargs": "^17.0.35",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
